{
  "name": "bugbash",
  "license": "MIT",
  "description": "Easier way to manage bug bashes",
  "private": true,
  "main": "webpack.config.js",
  "version": "1.0.0",
  "scripts": {
    "clean": "rimraf dist *.vsix",
    "cleanall": "npm run clean && rimraf typings node_modules .sass-cache",
    "dev": "webpack-dev-server --hot --progress --colors --content-base ./src --https --port 8888",
    "build:release": "npm run clean && mkdir dist && webpack --progress --colors --output-path ./dist -p",
    "package:dev": "node ./cmd_scripts/packageDev",
    "package:release": "node ./cmd_scripts/packageRelease",    
    "postinstall": "typings install"
  },
  "devDependencies": {
    "@types/react": "^15.0.21",
    "@types/react-dom": "^0.14.23",
    "@types/jquery": "^2.0.41",
    "@types/q": "^1.0.0",
    "webpack": "^2.3.1",
    "webpack-dev-server": "^2.4.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "css-loader": "^0.28.0",
    "ts-loader": "^2.0.3",
    "node-sass": "^4.5.2",
    "tfx-cli": "^0.4.1",
    "typescript": "^2.2.2",
    "typings": "^2.1.0",
    "uglifyjs-webpack-plugin": "^0.4.0",
    "copy-webpack-plugin": "^4.0.1"
  },
  "dependencies": {
    "vss-web-extension-sdk": "^2.114.0",
    "office-ui-fabric-react": "^2.10.4",
    "bootstrap": "^3.3.7",
    "jquery": "^3.2.1",
    "summernote": "^0.8.2",
    "es6-promise": "^4.1.0"
  }
}